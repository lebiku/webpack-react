# EditorConfig is awesome: http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_style = space
indent_size = 2
end_of_line = crlf
charset = utf-8
triming_trailing_whitespaces = true
insert_final_newline = true


[*.md]
triming_trailing_whitespaces = false
